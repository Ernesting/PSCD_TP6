Gestor de colas

protocolo de mensaje (Accion,cola,Twet) para master->gestor
					 (Accion,cola) para worker->gestor
					 (Accion,cola, datos) par gestor->worker
					 (Accion,cola) para cliente->gestor
					 (Accion, cola, datos) gestor->cliente
					 
//NOTA hay cosas que pueden cambiar ya que ns como estan implementado los demas procesos

GestorColas(){
	GestorMaster()
	GestorStreaming()
	GestorCliente()
	GestorWorker()
	
GestorMaster(master_fd, ControlColas){
	while(accion!=FIN){
		Recibirmensaje(master_fd, mensaje)
		if(accion=AÃ±adirTarea){
			ControlColas.publish(cola,Twet)
		}
		Mandarmensaje(master_fd,OK)
	}
}

GestorWorker(worfer_fd, ControlColas){
	while(accion!=fin){
		Recibirmensaje(worker_fd, mensaje)
		if(accion==ProcesarTarea){
			ControlColas.read(cola, x) ---------> podemos suponer que la cola es Tareas?
			Mandarmensaje(worker_fd,mensaje)
			}
		if(accion==GuardarDatos){
			ControlColas.publish(cola,datos)
			Mandarmensaje(worker_fd, "Datos guardados")
		}
	}
GestorCliente(cliente_fd, ControlColas){
	while(accion!=fin){
		Recibirmensaje(cliente_fd,mensaje)
		if(accion=Sacardatos){
			ControlColas.read(cola,datos)
			Mandarmensaje(cliente_fd,mensaje)
		}
	}
}


	
	
	
	
